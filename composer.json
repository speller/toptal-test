{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3.0",
        "nelmio/cors-bundle": "^1.5",
        "pear/http_request2": "^2.3",
        "spiral/roadrunner": "^1.3",
        "symfony/console": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/psr-http-message-bridge": "^1.1",
        "symfony/web-server-bundle": "4.2.*",
        "symfony/yaml": "4.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/dotenv": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
